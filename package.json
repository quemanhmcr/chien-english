{
  "name": "chien-english",
  "private": true,
  "version": "1.0.0",
  "description": "Interactive English learning platform with AI-powered lessons and feedback",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "npm run build && wrangler pages deploy dist --project-name=chien-english",
    "deploy:preview": "npm run build && wrangler pages deploy dist --project-name=chien-english --branch=preview",
    "cf:login": "wrangler login",
    "cf:secret": "wrangler pages secret put --project-name=chien-english",
    "cf:secrets:list": "wrangler pages secret list --project-name=chien-english",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@google/genai": "^1.33.0",
    "@openrouter/sdk": "^0.3.10",
    "@supabase/supabase-js": "^2.89.0",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.560.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251228.0",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "dotenv": "^17.2.3",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "wrangler": "^4.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
